#include<stdio.h>
#define N 456
#define X 124.35
#define S "Hello World"
int main(void)
{
  printf("*%8d*\n", N);
  printf("*%-8d*\n", N);
  printf("*%.2f*\n", X);
  printf("*%10.3f*\n", X);
  printf("*%-10.3e*\n", X);
  printf("*%+10.2f*\n", X);
  printf("*%010.2f*\n", X);
  printf("*%15s*\n", S);
  printf("*%15.5s*\n", S);
  printf("*%-15.5s*\n", S);

  return 0;
}
